{
  "name": "pc-mr",
  "version": "1.0.0",
  "description": "PC Zubehör Shop",
  "main": "index.html",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build --prod",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:fix": "npm run lint:scriptfix && npm run lint:stylefix",
    "lint:script": "eslint --ext .js --color --cache --cache-location tmp/.eslintcache --ignore-path .gitignore .",
    "lint:scriptfix": "npm run lint:script -- --fix",
    "lint:style": "stylelint \"**/*.{css,sass,scss}\" --ignore-path .gitignore --max-warnings 0",
    "lint:stylefix": "npm run lint:style -- --fix",
    "format": "prettier --write \"${FOLDER:-*}/**/*.*\""
  },
  "keywords": [
    "shop"
  ],
  "author": "Aleksander Vuk Jovanovic, Fynn Lohse, Jonas Pfannkuche, Tayfun Gülbasi, Vera Meermeier",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "del": "^5.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-file-include": "^2.2.2",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "vinyl-named": "^1.1.0",
    "webpack-stream": "^5.2.1",
    "yargs": "^15.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.*": [
      "npm run format"
    ],
    "*.{js}": [
      "npm run lint:scriptfix"
    ],
    "*.{css,sass,scss}": [
      "npm run lint:stylefix"
    ]
  }
}
