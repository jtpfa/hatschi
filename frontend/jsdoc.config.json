{
  "opts": {
    "template": "node_modules/vue-x-docs",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": ["node_modules/jsdoc-vue", "node_modules/vue-x-docs"],
  "source": {
    "include": ["components", "helpers", "layouts", "pages", "plugins", "schemes", "store"],
    "includePattern": "\\.(vue|js)$",
    "exclude": ["node_modules"]
  }
}